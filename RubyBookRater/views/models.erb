<h3>Current Ratings</h3>
<table class='table table-hover table-striped'>
<h4>Book Name:Average Rating Table</h4>
<% @models.each do |model| %>
	<tr>
		<td> <%= model.name %> </td>
		<th><%= model.getAverage("")[0] %></th>
		<td> <%= model.rating %> </td>
	</tr>
<% end %>
<a href="/">Back to Home</a>